<resources>

    <style name="cell_divider_hor">
        <item name="android:background">#e6e6e6</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
    </style>

    <style name="ActionBtnBg">
        <item name="android:background">@drawable/bg_black_a10_selector_round</item>
    </style>

    <style name="AppCompatDialog" parent="Theme.AppCompat.Light.Dialog.Alert" />

    <!-- 圆角按钮样式,项目常用的通用样式,颜色为橘红色 -->
    <style name="RoundCornerBtnBg">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/round_corner_btn_bg</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/btn_text_white_color</item>
    </style>

    <!-- 圆角边框有颜色按钮样式,项目常用的通用样式,颜色为橘红色 -->
    <style name="RoundCornerBorderBtnBg">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">32dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/round_corner_border_btn_bg</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/btn_text_orange_color</item>
    </style>

    <style name="LoginGetAutoCodeBg">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">32dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/login_get_auto_code_btn_bg</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/btn_text_orange_color</item>
    </style>

    <style name="LoginRetryGetAutoCodeBg">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">32dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/login_retry_get_code_btn_bg</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#757575</item>
    </style>

    <style name="AppCompatDialog2" parent="AppCompatDialog">
        <item name="android:windowAnimationStyle">@style/SubMenuPanel</item>
    </style>

    <style name="text_14_333333_e84a01">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/filter_house_text_color_selector</item>
    </style>


    <style name="text_14_c0c0c0_c20c0c">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@drawable/finance_home_btn_text_color</item>
    </style>

    <style name="CustomPopAnimOnMiddle">
        <item name="android:windowEnterAnimation">@anim/anim_pop_start_top_middle</item>
        <item name="android:windowExitAnimation">@anim/anim_pop_end_top_middle</item>
    </style>

    <style name="CustomPopAnimOnRight">
        <item name="android:windowEnterAnimation">@anim/anim_pop_start_top_right</item>
        <item name="android:windowExitAnimation">@anim/anim_pop_end_top_right</item>
    </style>

    <style name="CustomDialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimAmount">0.4</item>
    </style>
    <style name="textview_openaccount_current_step_center">
        <item name="android:textSize">26sp</item>
        <item name="android:textColor">#f75a14</item>
    </style>
    <style name="textview_openaccount_current_step_left">
        <item name="android:textSize">26sp</item>
