<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/global_background"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/mine_top_margin_bg_scroll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="170dp"
            android:gravity="center"
            android:orientation="vertical" />

    </LinearLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:paddingBottom="13dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:paddingTop="13dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:orientation="vertical">

            <TextView
                style="@style/text_12_212121"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="账户可用余额 (元)" />

            <TextView
                style="@style/text_16_212121"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:text="0.00" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_account_balance_get_cash"
                android:layout_width="72dp"
                android:layout_height="30dp"
                android:background="@drawable/outer_f75a14_radius_100_bg"
                android:gravity="center"
                android:text="提现"
                android:textColor="#f75a14"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_account_balance_charge"
                android:layout_width="72dp"
                android:layout_height="30dp"
                android:layout_marginLeft="16dp"
                android:background="@drawable/fill_f75a14_radius_100_bg"
                android:gravity="center"
                android:text="充值"
                android:textColor="@color/white"
                android:textSize="14sp" />
        </LinearLayout>

    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <View style="@style/cell_divider_hor" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="48dip"
            android:background="@drawable/white_to_pressed_selector"
            android:clickable="true"
            android:paddingLeft="16dp"
            android:paddingRight="16dp">

            <TextView
                style="@style/text_14_757575"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:text="2笔共190000.00元" />


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    style="@style/text_14_757575"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="申购中" />

                <View
                    android:layout_width="8dp"
                    android:layout_height="16dp"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/account_arrow_selector" />
            </LinearLayout>

        </RelativeLayout>


    </LinearLayout>

    <View
        style="@style/cell_divider_hor"
        android:layout_marginBottom="8dp" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="48dip"
        android:background="@drawable/white_to_pressed_selector"
        android:paddingLeft="16dp"
        android:paddingRight="16dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/mine_gift" />

            <TextView
                android:id="@+id/textView"
                style="@style/text_14_212121"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:text="邀请奖励" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                style="@style/text_14_757575"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="待发收益12.80元" />

            <View
                android:layout_width="8dp"
                android:layout_height="16dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/account_arrow_selector" />
        </LinearLayout>

    </RelativeLayout>

    <View
        style="@style/cell_divider_hor"
        android:layout_marginBottom="8dp" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="48dip"
        android:background="@drawable/white_to_pressed_selector"
        android:paddingLeft="16dp"
        android:paddingRight="16dp">

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_close_wallet"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/white_to_pressed_selector"
        android:paddingBottom="18dp"
        android:paddingLeft="16dp"
        android:paddingTop="18dp"
        android:visibility="visible">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:text="钱包"
                android:textColor="#757575"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tvWalletSum"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:textColor="#212121"
                android:textSize="22sp" />

        </LinearLayout>

        <View
            android:layout_width="8dp"
            android:layout_height="16dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="16dp"
            android:background="@drawable/account_arrow_selector" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_new_balance"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:visibility="visible">

        <LinearLayout
            android:id="@+id/ll_total_balance"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_12"
            android:paddingLeft="@dimen/_16"
            android:paddingRight="@dimen/_16"
            android:paddingTop="@dimen/_12">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的资产(元)"
                    android:textColor="#757575"
                    android:textSize="12sp" />


            </LinearLayout>

            <TextView
                android:id="@+id/tv_total_balance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0.00"
                android:textColor="#212121"
                android:textSize="18sp" />
        </LinearLayout>


        <View
            android:id="@+id/line_balance"
            style="@style/cell_divider_hor"
            android:layout_below="@+id/ll_total_balance" />

        <LinearLayout
            android:id="@+id/llBalance"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/line_balance"
            android:orientation="horizontal">

            <RelativeLayout
                android:id="@+id/rl_wallet"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/white_to_pressed_selector"
                android:padding="@dimen/_16">

                <com.ailicai.app.widget.TextViewTF
                    android:id="@+id/tvWalletIcon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/mine_piggybank"
                    android:textColor="#5dc3f3"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tv_wallet"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_toRightOf="@+id/tvWalletIcon"
                    android:text="活期宝"
                    android:textColor="#212121"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/tv_wallet_balance"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/tv_wallet"
                    android:layout_below="@+id/tv_wallet"
                    android:layout_marginTop="@dimen/_6"
                    android:text="0.00"
                    android:textColor="#757575"
                    android:textSize="14sp" />

            </RelativeLayout>

            <View
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="#e6e6e6" />

            <RelativeLayout
                android:id="@+id/rl_deposit"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/white_to_pressed_selector"
                android:padding="@dimen/_16">

                <com.ailicai.app.widget.TextViewTF
                    android:id="@+id/tvFinancingIcon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="1dp"
                    android:text="@string/personal_financing"
                    android:textColor="#f7683f"
                    android:textSize="22sp" />

                <TextView
                    android:id="@+id/tv_deposit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_toRightOf="@+id/tvFinancingIcon"
                    android:text="爱理财"
                    android:textColor="#212121"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/tv_deposit_balance"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/tv_deposit"
                    android:layout_below="@+id/tv_deposit"
                    android:layout_marginTop="@dimen/_6"
                    android:text="预计年化超7%-9%"
                    android:textColor="#757575"
                    android:textSize="14sp" />

            </RelativeLayout>

        </LinearLayout>

        <View
            android:id="@+id/lineCard"
            style="@style/cell_divider_hor"
            android:layout_below="@+id/llBalance" />

        <LinearLayout
            android:id="@+id/llCard"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lineCard"
            android:orientation="horizontal">

            <RelativeLayout
                android:id="@+id/mineCardCouponst"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/white_to_pressed_selector"
                android:padding="@dimen/_16">

                <com.ailicai.app.widget.TextViewTF
                    android:id="@+id/tvCardTicketIcon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/personal_ticket"
                    android:textColor="#fec331"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tvCardTicket"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_toRightOf="@+id/tvCardTicketIcon"
                    android:text="卡券"
                    android:textColor="#212121"
                    android:textSize="16sp" />

                <ImageView
                    android:id="@+id/ticket_red_dot"
                    android:layout_width="8dp"
                    android:layout_height="8dp"
                    android:layout_marginRight="4dp"
                    android:layout_toRightOf="@+id/tvCardTicket"
                    android:src="@drawable/follow_new_point" />

                <TextView
                    android:id="@+id/tvCardCouponsCount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/tvCardTicket"
                    android:layout_below="@+id/tvCardTicket"
                    android:layout_marginTop="@dimen/_6"
                    android:text="0张"
                    android:textColor="#757575"
                    android:textSize="14sp" />

            </RelativeLayout>

            <View
                android:layout_width="1px"
                android:layout_height="match_parent"
                android:background="#e6e6e6" />

            <RelativeLayout
                android:id="@+id/rl_bank_card"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/white_to_pressed_selector"
                android:padding="@dimen/_16">

                <com.ailicai.app.widget.TextViewTF
                    android:id="@+id/tvBankCardIcon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/personal_card"
                    android:textColor="#44d287"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tvBankCard"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_toRightOf="@+id/tvBankCardIcon"
                    android:text="银行卡"
                    android:textColor="#212121"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/tvBankCardCount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/tvBankCard"
                    android:layout_below="@+id/tvBankCard"
                    android:layout_marginTop="@dimen/_6"
                    android:text="0张"
                    android:textColor="#757575"
                    android:textSize="14sp" />

            </RelativeLayout>

        </LinearLayout>

        <View
            android:id="@+id/lineCardEnd"
            style="@style/cell_divider_hor"
            android:layout_below="@+id/llCard" />

    </RelativeLayout>

    <View
        style="@style/cell_divider_hor"
        android:layout_marginTop="8dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <RelativeLayout
            android:id="@+id/rl_yue_kan_list"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/white_to_pressed_selector"
            android:clickable="true"
            android:gravity="center|left">

            <LinearLayout
                android:id="@+id/llYuekan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingBottom="16dp"
                android:paddingTop="16dp">

                <com.ailicai.app.widget.TextViewTF
                    android:id="@+id/yue_kan_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/mine_list"
                    android:textColor="#616161"
                    android:textSize="26sp" />

                <TextView
                    style="@style/text_12_212121"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/yue_kan_icon"
                    android:layout_marginTop="8dp"
                    android:text="约看清单" />

            </LinearLayout>

            <TextView
                android:id="@+id/tv_yue_kan_count"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignRight="@+id/llYuekan"
                android:layout_alignTop="@+id/llYuekan"
                android:layout_marginRight="-6dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/msg_new_more"
                android:gravity="center"
                android:text="99+"
                android:textColor="@color/white"
                android:textSize="10sp"
                android:visibility="gone" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_kan_fang"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/white_to_pressed_selector"
            android:clickable="true"
            android:gravity="center">

            <LinearLayout
                android:id="@+id/llKanfang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingBottom="16dp"
                android:paddingTop="16dp">

                <com.ailicai.app.widget.TextViewTF
                    android:id="@+id/kan_fang_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/mine_schedule"
                    android:textColor="#616161"
                    android:textSize="26sp" />

                <TextView
                    android:id="@+id/textView8"
                    style="@style/text_12_212121"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/yue_kan_icon"
                    android:layout_marginTop="8dp"
                    android:text="看房日程" />

            </LinearLayout>

            <TextView
                android:id="@+id/tv_kan_fang_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignRight="@+id/llKanfang"
                android:layout_alignTop="@+id/llKanfang"
                android:layout_marginRight="-12dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/bg_red_solid_corner_10dp"
                android:gravity="center"
                android:paddingBottom="2dp"
                android:paddingLeft="4dp"
                android:paddingRight="4dp"
                android:paddingTop="2dp"
                android:text="待评价"
                android:textColor="@color/white"
                android:textSize="10sp"
                android:visibility="gone" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rlAgent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/white_to_pressed_selector"
            android:clickable="true"
            android:gravity="center">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingBottom="16dp"
                android:paddingTop="16dp">

                <com.ailicai.app.widget.TextViewTF
                    android:id="@+id/tvAgent"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/accountagent"
                    android:textColor="#616161"
                    android:textSize="26sp" />

                <TextView
                    style="@style/text_12_212121"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tvAgent"
                    android:layout_marginTop="8dp"
                    android:text=" 经纪人" />

            </LinearLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/mine_my_order"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/white_to_pressed_selector"
            android:clickable="true"
            android:gravity="center">


            <LinearLayout
                android:id="@+id/llOrder"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingBottom="16dp"
                android:paddingTop="16dp">

                <com.ailicai.app.widget.TextViewTF
                    android:id="@+id/tvOrder"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/mine_orders"
                    android:textColor="#616161"
                    android:textSize="26sp" />

                <TextView
                    android:id="@+id/textView7"
                    style="@style/text_12_212121"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tvOrder"
                    android:layout_marginTop="8dp"
                    android:text="合同订单" />

            </LinearLayout>

            <TextView
                android:id="@+id/order_state_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignRight="@+id/llOrder"
                android:layout_alignTop="@+id/llOrder"
                android:layout_marginRight="-12dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/bg_red_solid_corner_10dp"
                android:gravity="center"
                android:paddingBottom="2dp"
                android:paddingLeft="4dp"
                android:paddingRight="4dp"
                android:paddingTop="2dp"
                android:text="新账单"
                android:textColor="@color/white"
                android:textSize="10sp"
                android:visibility="gone" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/house_brower"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/white_to_pressed_selector"
            android:clickable="true"
            android:gravity="center|right">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="16dp"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingBottom="16dp"
                android:paddingTop="16dp">

                <com.ailicai.app.widget.TextViewTF
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/house_empty"
                    android:textColor="#616161"
                    android:textSize="26sp" />

                <TextView
                    style="@style/text_12_212121"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="浏览历史" />

            </LinearLayout>

        </RelativeLayout>

    </LinearLayout>

    <View
        style="@style/cell_divider_hor"
        android:layout_marginBottom="8dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <View style="@style/cell_divider_hor" />

        <RelativeLayout
            android:id="@+id/mine_entrust_manage_list"
            android:layout_width="match_parent"
            android:layout_height="48dip"
            android:background="@drawable/white_to_pressed_selector"
            android:clickable="true"
            android:paddingLeft="16dp"
            android:paddingRight="16dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/mine_owner_text"
                android:textColor="#de212121"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tvOwnerCount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="8dp"
                android:layout_toLeftOf="@+id/owner_arrow_order"
                android:text=""
                android:textColor="#de757575" />

            <View
                android:id="@+id/owner_arrow_order"
                android:layout_width="8dp"
                android:layout_height="16dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@drawable/account_arrow_selector" />

        </RelativeLayout>

        <View style="@style/cell_divider_hor" />

    </LinearLayout>

    <View
        style="@style/cell_divider_hor"
        android:layout_marginBottom="8dp" />

    <!--品牌公寓约看记录-->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <View style="@style/cell_divider_hor" />

        <RelativeLayout
            android:id="@+id/rl_brand_department_appointment_record"
            android:layout_width="match_parent"
