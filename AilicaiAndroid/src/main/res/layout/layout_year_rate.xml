<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/tv_no_data"
        android:layout_marginTop="13dp"
        android:layout_marginBottom="13dp"
        android:layout_marginLeft="@dimen/_16"
        android:text="没有匹配的房产宝"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#757575"
        android:textSize="14sp"/>
    <LinearLayout
        android:visibility="gone"
        android:id="@+id/ll_container"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <FrameLayout
            android:layout_marginTop="@dimen/_16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/tv_count"
                android:layout_marginLeft="@dimen/_16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#757575"
                android:textSize="14sp"/>
            <LinearLayout
                android:layout_marginRight="16dp"
                android:layout_gravity="right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone">
                <TextView
                    android:text="详情"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#757575"
                    android:textSize="14sp"/>
                <com.ailicai.app.widget.TextViewTF
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4dp"
                    android:text="@string/arrow_right"
                    android:textColor="#bdbdbd"
                    android:textSize="12sp" />
            </LinearLayout>
        </FrameLayout>

        <com.ailicai.app.ui.reserve.YearRateLayout
            android:layout_marginTop="@dimen/_16"
            android:layout_marginLeft="16dp"
            android:id="@+id/year_rate_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        </com.ailicai.app.ui.reserve.YearRateLayout>

        <TextView
            android:id="@+id/tv_preserve_buy_time"
            android:gravity="center_vertical"
            android:text="预约成功后，%1s将为您申购以上房产宝"
            android:textSize="12sp"
            android:textColor="#757575"
            android:paddingBottom="@dimen/_10"
            android:paddingTop="@dimen/_10"
            android:paddingLeft="@dimen/_16"
            android:layout_width="match_parent"
            android:background="#f5f8fa"
            android:layout_height="@dimen/_40" />
    </LinearLayout>
</FrameLayout>